ul.wgt-mega-menu
{ 
	line-height: 16px;
}

ul.wgt-mega-menu ul, 
.wgt-mega-menu ul li 
{
	list-style: none;
}

ul.wgt-mega-menu ul 
{
	position: relative;
	padding: 0; 
	margin: 0;
}

ul.wgt-mega-menu ul li ul 
{
	display: none;
}

ul.wgt-mega-menu .sub 
{
	display: none;
}

ul.wgt-mega-menu .sub ul 
{
	display: block;
}


ul.wgt-mega-menu, 
ul.wgt-mega-menu li 
{
	margin: 0; 
	padding: 0; 
	border: none;
}

ul.wgt-mega-menu 
{
	/*background: #222 url(http://127.0.0.1//WorkspaceWebFrap/WebFrap_Wgt/example/gw/../../themes/default/images/mega_menu/bg_light_blue.png) repeat-x 0 -80px; */
	width: 100%; 
	height: 40px; 
	border-right: 1px solid #44B3FF; 
	border-left: 1px solid #44B3FF; 
	position: relative;
}

ul.wgt-mega-menu li 
{
	float: left; 
	margin: 0; 
	padding: 0; 
	font-size: 13px; 
	font-weight: bold;
}

ul.wgt-mega-menu li>a,
ul.wgt-mega-menu li>p
{
	float: left; 
	display: block; 
	color: #000; 
	padding: 12px 17px 12px 15px; 
	/*background: url(http://127.0.0.1//WorkspaceWebFrap/WebFrap_Wgt/example/gw/../../themes/default/images/mega_menu/bg_light_blue.png) repeat-x 100% 0;*/
	text-shadow: 1px 1px 1px #fff; 
	text-decoration: none;
}

ul.wgt-mega-menu li a.wgt-mega,
ul.wgt-mega-menu li p.wgt-mega
{
	position: relative;
}

ul.wgt-mega-menu li a .wgt-mega-icon 
{
	display: block; 
	position: absolute;  
	top: 18px; 
	right: 15px; 
	width: 8px; 
	height: 6px; 
	background: url(http://127.0.0.1//WorkspaceWebFrap/WebFrap_Wgt/example/gw/../../themes/default/images/mega_menu/arrow.png) no-repeat 0 100%;
}

ul.wgt-mega-menu li.mega-hover a, 
ul.wgt-mega-menu li>a:hover,
ul.wgt-mega-menu li.mega-hover p, 
ul.wgt-mega-menu li>p:hover 
{
	background-position: 100% -40px; 
	color: #000; 
	text-shadow: none;
}

ul.wgt-mega-menu li.mega-hover a .wgt-mega-icon 
{
	background-position: 0 100%;
}

ul.wgt-mega-menu li .sub-container 
{
	position: absolute; 
	background: url(http://127.0.0.1//WorkspaceWebFrap/WebFrap_Wgt/example/gw/../../themes/default/images/mega_menu/bg_sub_left.png) no-repeat 0 100%; 
	padding-left: 20px; 
	margin-left: -3px;
}

ul.wgt-mega-menu li .sub 
{
	background: url(http://127.0.0.1//WorkspaceWebFrap/WebFrap_Wgt/example/gw/../../themes/default/images/mega_menu/bg_sub.png) no-repeat 100% 100%; 
	padding: 20px 20px 20px 10px;
}

ul.wgt-mega-menu li .sub-container.mega .sub 
{
	padding: 20px 20px 10px 0;
}

ul.wgt-mega-menu li .sub .row 
{
	width: 100%; 
	overflow: hidden; 
	clear: both;
}

ul.wgt-mega-menu li .sub li 
{
	list-style: none; 
	float: none; 
	width: 170px; 
	font-size: 1em; 
	font-weight: normal;
}

ul.wgt-mega-menu li .sub li.mega-hdr 
{
	margin: 0 10px 10px 0; 
	float: left;
}

ul.wgt-mega-menu li .sub li.mega-hdr.last 
{
	margin-right: 0;
}

ul.wgt-mega-menu li .sub a,
ul.wgt-mega-menu li .sub p
{
	background: none; 
	border: none; 
	text-shadow: none; 
	color: #000000; 
	padding: 7px 10px; 
	display: block; 
	float: none; 
	text-decoration: none; 
	font-size: 0.9em;
}

ul.wgt-mega-menu li .sub li.mega-hdr a.mega-hdr-a,
ul.wgt-mega-menu li .sub li.mega-hdr p.mega-hdr-a 
{
	padding: 5px 5px 5px 15px; 
	margin-bottom: 5px; 
	background: #4EC3F3 url(http://127.0.0.1//WorkspaceWebFrap/WebFrap_Wgt/example/gw/../../themes/default/images/mega_menu/bg_mega_hdr.png) no-repeat 0 0; 
	text-transform: uppercase; 
	font-weight: bold; 
	color: #000000; 
	text-shadow: 1px 1px 1px #fff;
}

ul.wgt-mega-menu li .sub li.mega-hdr a.mega-hdr-a:hover,
ul.wgt-mega-menu li .sub li.mega-hdr p.mega-hdr-a:hover 
{
	color: #fff; 
	text-shadow: none;
}

ul.wgt-mega-menu .sub li.mega-hdr li a,
ul.wgt-mega-menu .sub li.mega-hdr li p
{
	padding: 4px 5px 4px 20px; 
	background: url(http://127.0.0.1//WorkspaceWebFrap/WebFrap_Wgt/example/gw/../../themes/default/images/mega_menu/arrow_off.png) no-repeat 5px 8px; 
	font-weight: normal;
}

ul.wgt-mega-menu .sub li.mega-hdr li a:hover,
ul.wgt-mega-menu .sub li.mega-hdr li p:hover
{
	color: #a32403; 
	background: #efefef url(http://127.0.0.1//WorkspaceWebFrap/WebFrap_Wgt/example/gw/../../themes/default/images/mega_menu/arrow_on.png) no-repeat 5px 8px;
}

ul.wgt-mega-menu .sub ul li 
{
	padding-right: 0;
}

ul.wgt-mega-menu li .sub-container.non-mega .sub 
{
	padding: 20px 20px 20px 0;
}

ul.wgt-mega-menu li .sub-container.non-mega li 
{
	padding: 0; 
	width: 190px; 
	margin: 0;
}

ul.wgt-mega-menu li .sub-container.non-mega li a,
ul.wgt-mega-menu li .sub-container.non-mega li p 
{
	padding: 7px 5px 7px 22px; 
	background: url(http://127.0.0.1//WorkspaceWebFrap/WebFrap_Wgt/example/gw/../../themes/default/images/mega_menu/arrow_off.png) no-repeat 7px 10px;
}

ul.wgt-mega-menu li .sub-container.non-mega li a:hover,
ul.wgt-mega-menu li .sub-container.non-mega li p:hover
{
	color: #a32403; 
	background: #efefef url(http://127.0.0.1//WorkspaceWebFrap/WebFrap_Wgt/example/gw/../../themes/default/images/mega_menu/arrow_on.png) no-repeat 7px 10px;
}

/* Vertical Mega Menu Styles */
.wgt-vert-mega-menu
{
  line-height: 16px; 
  background: #EAEAEA;
  border-left: 1px solid #1B1B1B; 
  position: relative; /* Required */
}

.wgt-vert-mega-menu li a 
{
  display: block; 
  color: #fff; 
  padding: 10px 38px 10px 25px;
  text-shadow: 1px 1px 1px #000; 
  text-decoration: none;  
  border-top: 1px solid #555; 
  border-bottom: 1px solid #222; 
  border-right: 1px solid #1B1B1B;
}

.wgt-vert-mega-menu li a:hover, 
.wgt-vert-mega-menu li.mega-hover a 
{
  background: #0068A5;
  color: #fff; 
  border-right: 1px solid #0068A5;
}

.wgt-vert-mega-menu li a.wgt-mega 
{
  position: relative;
}

/* Add arrow icon to parent links */
.wgt-vert-mega-menu li a .wgt-mega-icon 
{
  display: block; 
  position: absolute; 
  top: 18px; 
  right: 15px; 
  width: 6px; 
  height: 8px; 
  background: url(http://127.0.0.1//WorkspaceWebFrap/WebFrap_Wgt/example/gw/../../themes/default/images/wgt/arrow_right.png) no-repeat 0 0;
}

/* Mega menu container */
.wgt-vert-mega-menu li .sub-container 
{
  position: absolute; /* Required */
  background: #EAEAEA;
  padding: 10px 10px 0 10px; 
  border: 1px solid #4b4b4b; 
  border-left: none;
}

.wgt-vert-mega-menu li .sub-container .sub {}

.wgt-vert-mega-menu li .sub .row 
{
  width: 100%; overflow: hidden; /* Clear floats */
}

.wgt-vert-mega-menu li .sub li 
{
  float: none; 
  width: 150px; 
  font-size: 1em; 
  font-weight: normal;
}

.wgt-vert-mega-menu li .sub li.mega-hdr 
{
  float: left; /* Required */
  margin: 0 5px 10px 5px;
}

.wgt-vert-mega-menu li .sub a, .wgt-vert-mega-menu.left li .sub a 
{
  background: none; 
  border: none; 
  text-shadow: none;
  float: none;
  color: #000;
  padding: 7px 10px; 
  display: block;
  text-decoration: none; 
  font-size: 0.9em;
}

.wgt-vert-mega-menu li .sub li.mega-hdr a.mega-hdr-a 
{
  padding: 5px 5px 5px 15px; 
  margin-bottom: 5px; 
  background: #0068A5;
  text-transform: uppercase; 
  font-weight: bold; 
  color: #fff; 
  text-shadow: 1px 1px 1px #333;
}

.wgt-vert-mega-menu li .sub li.mega-hdr a.mega-hdr-a:hover 
{
  color: #000; 
  text-shadow: none;
}

.wgt-vert-mega-menu .sub li.mega-hdr li a 
{
  padding: 4px 5px 4px 20px; 
  background: url(http://127.0.0.1//WorkspaceWebFrap/WebFrap_Wgt/example/gw/../../themes/default/images/wgt/arrow_white.png) no-repeat 5px 8px; 
  font-weight: normal;
}

.wgt-vert-mega-menu .sub li.mega-hdr li a:hover 
{
  color: #a32403; 
  background: #efefef url(http://127.0.0.1//WorkspaceWebFrap/WebFrap_Wgt/example/gw/../../themes/default/images/wgt/arrow_on.png) no-repeat 5px 8px;
}

.wgt-vert-mega-menu .sub ul li 
{
  padding-right: 0;
}

/* Styling for menu items with only 2 levels */
.wgt-vert-mega-menu li .sub-container.non-mega .sub 
{
  padding: 10px;
}

.wgt-vert-mega-menu li .sub-container.non-mega li 
{
  padding: 0; 
  margin: 0;
  width: 150px;
}

.wgt-vert-mega-menu li .sub-container.non-mega li a 
{
  padding: 7px 5px 7px 22px; 
  background: url(http://127.0.0.1//WorkspaceWebFrap/WebFrap_Wgt/example/gw/../../themes/default/images/wgt/arrow_white.png) no-repeat 7px 10px;
}

.wgt-vert-mega-menu li .sub-container.non-mega li a:hover 
{
  color: #a32403; 
  background: #efefef url(http://127.0.0.1//WorkspaceWebFrap/WebFrap_Wgt/example/gw/../../themes/default/images/wgt/arrow_on.png) no-repeat 7px 10px;
}
