}

/*//////////////////////////////////////////////////////////////////////////////
// Boxes for layouting
//////////////////////////////////////////////////////////////////////////////*/

.wgt-box
{
  padding-top:4px;
  padding-bottom:4px;
  padding-right:4px;
  height:100%;
}

/*
  Einfacher Container zum platzieren von bestimmten inhalten,
  zb container info
  container video... etc.
  Kann beliebig erweitert werden.
*/
div.wgt-container
{
  padding-top:4px;
  padding-bottom:4px;
  padding-right:4px;
  float:left;
}

/*
  Spezieller Container der die ganze zur verfügung stehende Breite
  reserviert.
  Ist fast identisch zu full, hat aber etwas mehr semantic.
  Im Gegensatz zu full darf wgt-slice customized werden
*/
div.wgt-slice
{
  width:100%;
  clear:both;
}

.wgt-center
{
  margin: 0px auto;
}

div.wgt-container.center
{
  position: absolute;
  top: 50%; 
  left: 50%;
  /* width und margin müssen selbst gesetzt werden
  width: 200px;
  height: 150px;
  margin-top: -75px;
  margin-left: -100px;
  */
}

.wgt-box.center.small
{
  width: 200px;
  height: 200px;
  margin-top: -100px;
  margin-left: -100px;
}

.wgt-box.center.medium
{
  width: 400px;
  height: 400px;
  margin-top: -200px;
  margin-left: -200px;
}

.wgt-box.center.large
{
  width: 600px;
  height: 600px;
  margin-top: -300px;
  margin-left: -300px;
}


div.wgt-seperator.vertical
{
  width:1px;
  border-right:1px solid black;
  height:98%;
  float:left;
}


/* is only a piece, has no borders an nothing, can be used to structure elements
* with no spaces togehter
*/
.wgt-piece
{
  height:100%;
}

table.fix_layout
{
  width:100%;
  height:100%;
  empty-cells:show;
  border-collapse:collapse;
  border-spacing:0px;
}



/*//////////////////////////////////////////////////////////////////////////////
// border
//////////////////////////////////////////////////////////////////////////////*/

/* @imutable */
.wgt-border
{
  border:1px solid silver;
}

.wgt-border-light
{
  border:2px groove silver;
}


.wgt-bg-light
{
  background-color:#F2F2F2;
}

/*//////////////////////////////////////////////////////////////////////////////
// rounded corners
//////////////////////////////////////////////////////////////////////////////*/


.wgt-corner
{
  border: 1px solid black;
  border-radius: 3px;
}
 
.wgt-corner-top
{
  border: 1px solid black;
  border-top-left-radius: 3px;
  border-top-right-radius: 3px;	
}
 
.wgt-corner-bottom
{
  border: 1px solid black;
  border-bottom-left-radius: 3px;
  border-bottom-right-radius: 3px;	
}

/*//////////////////////////////////////////////////////////////////////////////
// Backgrounds
//////////////////////////////////////////////////////////////////////////////*/

.wgt-bg-head,
.wgt-bg-foot
{
	/* Wird im Theme befüllt */
	background-color:silver;
}

/*//////////////////////////////////////////////////////////////////////////////
// Scrolling
//////////////////////////////////////////////////////////////////////////////*/

.wgt-scroll
{
   overflow-y:auto;
   overflow-x:auto;
}

.wgt-no-scroll
{
   overflow-y:hidden;
   overflow-x:hidden;
}

.wgt-scroll-y
{
   overflow-y:auto;
   overflow-x:hidden;
}

.wgt-scroll-x
{
   overflow-y:hidden;
   overflow-x:auto;
}



/*//////////////////////////////////////////////////////////////////////////////
// Pointers
//////////////////////////////////////////////////////////////////////////////*/

.wgt-cursor-pointer
{
	cursor:pointer;
}

.wgt-cursor-wait
{
  cursor:wait;
}

.wgt-cursor-info
{
  cursor:help;
}

.wgt-cursor-crosshair
{
  cursor:crosshair;
}

.wgt-cursor-text
{
  cursor:text;
}

/*//////////////////////////////////////////////////////////////////////////////
// opacity
//////////////////////////////////////////////////////////////////////////////*/

.wgt-opacity-10
{
  filter:alpha(opacity=10);
  -khtml-opacity: 0.1;
  opacity: 0.1;  
}

.wgt-opacity-20
{
  filter:alpha(opacity=20);
  -khtml-opacity: 0.2;
  opacity: 0.2;  
}

.wgt-opacity-30
{
  filter:alpha(opacity=30);
  -khtml-opacity: 0.3;
  opacity: 0.3;  
}

.wgt-opacity-40
{
  filter:alpha(opacity=40);
  -khtml-opacity: 0.4;
  opacity: 0.4;  
}

.wgt-opacity-50
{
  filter:alpha(opacity=50);
  -khtml-opacity: 0.5;
  opacity: 0.5;  
}

.wgt-opacity-60
{
  filter:alpha(opacity=60);
  -khtml-opacity: 0.6;
  opacity: 0.6;  
}

.wgt-opacity-70
{
  filter:alpha(opacity=70);
  -khtml-opacity: 0.7;
  opacity: 0.7;  
}

.wgt-opacity-80
{
  filter:alpha(opacity=80);
  -khtml-opacity: 0.8;
  opacity: 0.8;  
}

.wgt-opacity-90
{
  filter:alpha(opacity=90);
  -khtml-opacity: 0.9;
  opacity: 0.9;  
}

.wgt-opacity-100
{
  filter:alpha(opacity=100);
  -khtml-opacity: 1;
  opacity: 1;  
}

