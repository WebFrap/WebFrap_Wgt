@charset "utf-8";
/*******************************************************************************
* WebFrap Layout Template
*******************************************************************************/

/* clean all */
*
{
  margin:   0;
  padding:  0;
  font-size:12px;
  text-decoration:none;	
}

ul,li,ol
{	
  list-style:  none;	
}


body, html
{
  height:    100%;
  width:     100%;
  margin:    0px;
  padding:   0px;
}

a
{
  cursor:pointer;
}



/*//////////////////////////////////////////////////////////////////////////////
// Meta elements
//////////////////////////////////////////////////////////////////////////////*/

/* @imutable */
.template,
.meta,
.hidden,
.settings,
var
{
  /* Markierungsklasse für Vorlagen, wir vor dem Anzeigen gelöscht */
  display: none;
}

/*
Template welches für absolut positinierte Boxen verwendet wird
*/
.box_template
{
  display: none;
  position:absolute;
  z-index:5000;
  background-color:white;
  border:2px solid blue;
}


/*//////////////////////////////////////////////////////////////////////////////
// Layouting and positioning
//////////////////////////////////////////////////////////////////////////////*/


/* @imutable */
.clear
{
  clear: both;
}

/* @imutable */
div.left,
fieldset.left,
ul.left,
span.left
{
	display:block;
  clear: both;
  float: left;
	zoom: 1; /* FU IE */
}

/* @imutable */
div.inline,
fieldset.inline,
ul.inline
{
  clear:none;
  display:inline-block; /* sicher ? */
  float:left;
  zoom: 1; /* FU IE */	
}

/* @imutable */
div.right,
fieldset.right,
ul.right,
span.right
{
  display:block;
  float:right;
  display:block;
  zoom: 1; /* FU IE */	
}

/* @imutable */
div.rightline,
ul.rightline
{
  float:right;
  display:inline-block;
  zoom: 1; /* FU IE */	
}

/* @imutable */
div.width-auto,
ul.width-auto
{
  width:auto !important;
}

/* @imutable */
div.height-auto,
ul.height-auto
{
  height:auto !important;
}

/* @imutable */
div.auto
{
  width:auto !important;
  height:auto !important;
}

/* @imutable */
.full
{
  clear: both;
  width:auto;
  display:block;
}

table.full,
ul.full
{
  width:100%;
}

/* blockelemente auf 100% enforcen, aber sicher stellen, dass padding
keinen strich durch die rechnung macht. */
.forcefull
{
	padding:0px;
	witdh:100%;
}

/* @imutable */
.clear_line
{
  clear: both;
  height:1px;
}

/* @imutable */
.center
{
  margin: 0px auto;
}

/* @imutable */
.text_center
{
  text-align: center;
}

/*//////////////////////////////////////////////////////////////////////////////
// Layouting and positioning
//////////////////////////////////////////////////////////////////////////////*/


.pointer,
.wgt-clickable,
.cursor
{
  cursor:pointer;
}


.wgt-dyn-center
{
  position: absolute;
  top: 50%;
  left: 50%;
  /* width und margin müssen selbst gesetzt werden
  width: 200px;
  height: 150px;
  margin-top: -75px;
  margin-left: -100px;
  */
}

/*
  klasse zum erstellen von ausenabständen in box elementen
  default margin sind 5px;
  Änderungen nur in theme.css
*/
.wgt-space
{
  margin:5px !important;
}

.wgt-space p
{
  margin-left:0px;
	padding-left:0px;
}

.wgt-bspace
{
  margin-bottom:9px;
}

.wgt-bspace-s
{
  margin-bottom:5px;
}


