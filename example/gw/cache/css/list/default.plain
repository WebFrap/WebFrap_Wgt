/*//////////////////////////////////////////////////////////////////////////////
// Elemente zum erzeugen von Abständen
//////////////////////////////////////////////////////////////////////////////*/

div.indention.small
{
  clear:both;
  width:100%;
  margin-right:-10px;
}

